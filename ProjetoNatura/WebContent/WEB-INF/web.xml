<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>ProjetoNatura</display-name>
	<welcome-file-list>
		<welcome-file>Login.jsp</welcome-file>
	</welcome-file-list>

<servlet>
		<servlet-name>Cliente</servlet-name>
		<servlet-class>br.com.projetoNatura.servlets.ClienteServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Cliente</servlet-name>
		<url-pattern>/Cliente</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Produto</servlet-name>
		<servlet-class>br.com.projetoNatura.servlets.ProdutoServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Produto</servlet-name>
		<url-pattern>/Produto</url-pattern>
				
		
		
		
		
	</servlet-mapping>






</web-app>